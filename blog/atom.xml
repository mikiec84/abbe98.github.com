---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>

<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Albin Larsson</title>
  <link href="http://abbe98.github.io/blog/atom.xml" rel="self"/>
  <link href="http://abbe98.github.io/blog/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <author>
    <name>Albin Larsson</name>
    <email>albin.post@gmail.com</email>
  </author>{% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://abbe98.github.io/blog{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://abbe98.github.io/blog{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>{% endfor %}
</feed>